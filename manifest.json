{
"manifest_version": 2,
    "name": "Emojify",
    "description": "Replaces words and phrases with relevant emojis.",
    "version": "1.0",
    "icons": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
